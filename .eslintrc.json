{
	"extends": ["next", "next/core-web-vitals", "eslint:recommended", "plugin:prettier/recommended"],
	"env": {
		"browser": true,
		"node": true,
		"jest": true
	},
	"globals": {
		"React": "readonly"
	},
	"plugins": ["react", "@typescript-eslint", "prettier"],
	"rules": {
		"max-len": ["error", 140],
		"quotes": [2, "single", { "avoidEscape": true }]
	}
}
